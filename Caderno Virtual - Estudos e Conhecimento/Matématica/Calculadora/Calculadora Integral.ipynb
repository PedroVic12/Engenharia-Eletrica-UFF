   ],
   "source": [
    "a, b = smp.symbols('a b', real=True, positive=True)\n",
    "x = smp.symbols('x', real=True)\n",
    "f = smp.cos(b*x)* smp.exp(-a*x)\n",
    "f"
   ]
  },
  {
   "cell_type": "code",
